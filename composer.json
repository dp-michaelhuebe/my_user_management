{
    "name": "serfhos/my-user-management",
    "description": "TYPO3 Extension: A module that makes it possible for editors to maintain backend users.",
    "type": "typo3-cms-extension",

    "keywords": [
        "TYPO3",
        "CMS",
        "Serfhos",
        "Benjamin Serfhos",
        "Extbase",
        "User Management"
    ],
    "homepage": "https://github.com/bmoex/my_user_management",
    "license": "LGPL-3.0+",

    "version": "2.0.4",

    "autoload": {
        "psr-4": {
            "Serfhos\\MyUserManagement\\": "Classes/"
        }
    },

    "authors": [
        {
            "name": "Benjamin Serfhos",
            "email": "benjamin@serfhos.com",
            "homepage": "http://serfhos.com"
        }
    ],

    "require": {
        "composer/installers": "~1.0"
    },

    "replace": {
        "my_user_management": "self.version",
        "typo3-ter/my-user-management": "self.version"
    },

    "minimum-stability": "dev"
}